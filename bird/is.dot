    Mon [label = "Monoid"]
    PartEq [label = "PartialEquivalence"]
    RAbsorb [label = "RightAbsorbing\nx * 0 = 0"]
    RBinComm [label = "RightBinaryCommutative?\n- (x * y) = x * - y"]
