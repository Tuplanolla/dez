    TwoRLDistr [label = "RightLeftDistributive?\nx R* (a + b) = x R* a + x R* b"]
    TwoRRDistr [label = "RightRightDistributive?\n(x + y) R* a = x R* a + y R* a"]
    TwoRUnl [label = "RightUnital\nx R+ R0 = x"]
