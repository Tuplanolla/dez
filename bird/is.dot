    BilinForm [label = "BilinearForm"]
    BilinMap [label = "BilinearMapping"]
    BilinOp [label = "BilinearOperator"]
    LCompat [label = "LeftCompatible\na L+ (b L+ x) = (a + b) L+ x"]
